#include "main.h"
#include <stdio.h>
#include <stdlib.h>
/**
 *free_grid - function that frees a 2 dimensional grid
 *@grid:holds a grade
 *@height: a height
 */
void free_grid(int **grid, int height)
{
	int n;

	if (grid == NULL || height)
		return;
	for (n = 0; n < height; n++)
		free(grid[n];
		     free(grid);
}
